-if current_user.friends.any?
  - current_user.find_unconfirmed_friendships(current_user).each do |friend|
    %p Unconfirmed Friendships:
    - @user = User.find(friend)
    = link_to @user.first_name, confirm_friend_path(@user)
    = @user.last_name
    =
    %br

-@user_networks.each do |user_network|
  %tr
    %td
      = user_network.nickname
    %td
      = user_network.wifi_network_id
    %td
      = user_network.user_id
    %td
      = user_network.user_sharing_pref
    %td
      = user_network.user_score
    %td
      = link_to 'Show', user_network
    %td
      = link_to 'Edit', edit_user_network_path(user_network)
    %td
      = link_to 'Destroy', user_network, method: :delete, data: { confirm: 'Are you sure?' }

= link_to 'New User network', new_user_network_path
